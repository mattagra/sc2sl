<table>
  <tr>
    <th>Username</th>
    <th>Join Date</th>
    <th>Email</th>
    <th></th>
  </tr>
  <% @users.each do |user|  %>
  <tr>
    <td><%= link_to user.login, profile_path(user.login) %></td>
    <td><%= user.created_at %></td>
    <td><%= mail_to user.email %></td>
  </tr>
  <% end %>
</table>


Page: <%=  paginate_links(@per_page, @current_page, @users_count, request.path) %>